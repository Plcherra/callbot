# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Stripe (STRIPE_SECRET_KEY can be a restricted key, e.g. webhook-ai — see docs/STRIPE_PERMISSIONS.md)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_... or your restricted key
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRICE_ID=price_...
# Optional: from Stripe Payment Links → Buy button → Copy code (buy-button-id)
NEXT_PUBLIC_STRIPE_BUY_BUTTON_ID=

# Vapi.ai
VAPI_API_KEY=your_vapi_api_key
VAPI_PHONE_NUMBER_ID=optional_phone_number_id
VAPI_TEST_CALL_NUMBER=optional_test_number_for_dashboard
NEXT_PUBLIC_VAPI_TEST_CALL_NUMBER=optional_same_for_client

# Google OAuth
GOOGLE_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_client_secret
NEXT_PUBLIC_GOOGLE_REDIRECT_URI=http://localhost:3000/api/google/callback

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
